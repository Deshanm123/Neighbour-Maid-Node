  <!-- page title -->
  <title>NeighbourMaid | Login</title>
</head>

<body>
  <!-- navbar -->
  <!-- <nav class="navbar navbar-expand-lg navbar-light bg-light"> -->
  <nav class="navbar navbar-expand-lg">
    <!-- brand logo -->
    <a class="navbar-brand mr-auto pl-4" href="#"><img src="./NeighbourMaid Brand newcopy.png" alt="logo-maidfinder"
        width="120px"></a>
    <!-- nav content -->
    <div class="nav-content ">
      <button class="navbar-toggler" type="button" data-toggle="collapse" data-target="#navbarNav"
        aria-controls="navbarNav" aria-expanded="false" aria-label="Toggle navigation">
        <!--  -->
        <span class="navbar-toggler-icon"></span>
      </button>
      <div class="collapse navbar-collapse" id="navbarNav">
        <ul class="navbar-nav">
          <li class="nav-item active mx-4">
            <a class="nav-link" href="maid-dashboard.html"> My Dashboard <span class="sr-only">(current)</span></a>
          </li>
          <li class="nav-item mx-4">
            <a class="nav-link" href="./courses-list.html">Explore Courses</a>
          <li class="nav-item dropdown mx-5 pr-5">
            <a class="nav-link dropdown-toggle" data-toggle="dropdown" href="#" role="button" aria-haspopup="true"
              aria-expanded="false">
              <img src=".\img\profile.png" alt="" id="housemaid-navbar-profile" width="20px">
            </a>
            <div class="dropdown-menu">
              <a class="dropdown-item" href="/maid-account-registration.html">My Account</a>

              <div class="dropdown-divider"></div>
              <a class="dropdown-item" href="#">Log out</a>
            </div>
          </li>
        </ul>
      </div>
    </div>
  </nav>
  <!-- end of navbar -->




  <!-- courses grid -->
  <div class="container mt-5 mb-5">
    <div class="row">
      <div class="col-md-12">
        <div class="d-flex flex-row justify-content-between align-items-center filters">
          <h6>Showing <%= courses.length %> tasks</h6>
          <div class="right-sort">
            <div class="sort-by">
              <span class="mr-1">Sort by:</span><a href="#">Most popular</a><i class="fa fa-angle-down ml-1"></i>
              <button class="btn btn-outline-dark btn-sm ml-3 filter" type="button">Filters&nbsp;
                <i class="fa fa-flask"></i>
              </button>
            </div>
          </div>
        </div>
      </div>
    </div>
    <!-- courses view begin here -->
    <div class="row mt-1 pb-4">
      <% courses.forEach(course=>{ %>
        <!-- single course -->
        <div class="col-md-4 py-2">
          <!-- <div class="container"> -->
          <div class="container d-flex justify-content-end">

            <!-- edit:get action-->
            <a href="courses/editCourse/<%= course.courseId %>"><i class="fa fa-edit px-2" style="color:blue;"></i></a>

            <!-- remove:post action  -->
            <form method="POST" action="courses/remove/<%=course.courseId%>?_method=DELETE" class="d-inline">
              <button type="submit" class="btn btn-danger">Delete</button>
              <!-- <a href=""><i class="fa fa-trash-alt px-2" style="color:black;"></i></a> -->
            </form>

          </div>

          <!-- </div> -->
          <!-- course Tile -->
          <div class="p-card  p-1 rounded">
            <!-- img -->
            <!-- <img class="card-img-top img-responsive" src="..\..\public\img\courses\cleaning_2.jpg" alt="Card image cap"
              width="100px" height="200px"> -->
            <img class="card-img-top img-responsive" src="https://en.pimg.jp/069/675/674/1/69675674.jpg"
              alt="Card image cap" width="100px" height="200px">
            <!-- body -->
            <div class="card-body">
              <h5 class=" card-title mt-2">
                <%= course.courseTitle %>
              </h5>
              <span class="badge badge-danger py-1 mb-2">
                <%= course.courseCategory %>
              </span>
              <span class="d-block mb-5">
                <%= course.courseIntro %>
              </span>
              <a href="courses/view/<%= course.courseId %>" class="btn btn-primary w-100">Enroll Now</a>
              <!-- <a href="./courses/<course.courseId%>" class="btn btn-primary w-100">Enroll Now</a> -->
            </div>
          </div>
        </div>
        <!-- course end  -->
        <% }) %>
    </div>
  </div>
  <!-- pagination -->
  <div class="d-flex justify-content-end text-right mt-2">
    <nav>
      <ul class="pagination">
        <li class="page-item disabled">
          <a class="page-link" href="#" tabindex="-1">Previous</a>
        </li>
        <li class="page-item active">
          <a class="page-link" href="#">1 <span class="sr-only">(current)</span></a>
        </li>
        <li class="page-item"><a class="page-link" href="#">2</a></li>
        <li class="page-item"><a class="page-link" href="#">3</a></li>
        <li class="page-item"><a class="page-link" href="#">Next</a></li>
      </ul>
    </nav>
  </div>
  <!-- pagination end-->

  </div>
