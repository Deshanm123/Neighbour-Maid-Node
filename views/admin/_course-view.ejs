<!-- end of navbar -->
<% courses.forEach(course=>{ %>
  <% let publishedDate=new Date(course.courseDate).toDateString(); %>
    <!-- section course  -->
    <section id="course-section">
      <div class="container">
        <div class="row g-0 border rounded overflow-hidden flex-md-row mb-4 shadow-sm h-md-250 position-relative">
          <div class="col p-4 d-flex flex-column position-static">
            <span class="badge badge-danger py-1 mb-2">
              <%= course.courseCategory %>
            </span>
            <h1 class="display-4 font-weight-bold" id="course-title">
              <%= course.courseTitle %>
            </h1>
            <div class="mb-1 text-muted">
              <%= publishedDate %>
            </div>
            <p class="card-text mb-auto pt-3">
              <%= course.courseIntro %>
            </p>
            <!-- <button class="btn" onClick="Circle('.round');">Refresh</button> -->
          </div>
          <!-- <div class="d-none d-lg-block p-5" id="progress-section">
            <section id="ProgressCircleBar">
              <div class="container">
                <div class="row">

                  <div class="round" data-value="0.10" data-size="100" data-thickness="25">
                    <p>My progress <span>10%</span></p>
                  </div>
                </div>
              </div>
            </section>

          </div> -->
        </div>
      </div>
      </div>
      </div>
      <div class="container">
        <div class="embed-responsive embed-responsive-16by9 mt-5">
          <iframe width="884" height="497" src="https://www.youtube.com/embed/f0jVf_wRbJM" title="YouTube video player"
            frameborder="0"
            allow="accelerometer; autoplay; clipboard-write; encrypted-media; gyroscope; picture-in-picture"
            allowfullscreen></iframe>
        </div>
        <p class="pt-3">
        <h3>Course Description</h3>
        <%= course.courseDescription %>
          </p>
      </div>
      </div>
    </section>
    <!-- section course  -->
    <% }) %>
      <!-- course rating -->
      <section id="course-rating-section pb-3">
        <div class="container d-flex justify-content-center pb-5 pt-5">
          <div class="row">
            <div class="well well-sm">
              <div class="row">
                <div class="col-xs-12 col-md-6 text-center">
                  <!-- overole rating -->
                  <h1 class="rating-num"> 4.0</h1>
                  <div class="rating">
                    <span class="rstar"><i class="fas fa-star"></i></span>
                    <span class="rstar"><i class="fas fa-star"></i></span>
                    <span class="rstar"><i class="fas fa-star"></i></span>
                    <span class="rstar"><i class="fas fa-star"></i></span>
                    <span class="rstar-empty"><i class="far fa-star"></i></span>
                  </div>
                  <div>
                    <span class="ruser"><i class="fas fa-user"></i></span>1,050,008 total
                  </div>
                </div>
                <div class="col-xs-12 col-md-6">
                  <div class="row rating-desc">
                    <div class="col-xs-3 col-md-3 text-right">
                      <span class="rstar"></span>5
                    </div>
                    <div class="col-xs-8 col-md-9">
                      <div class="progress progress-striped">
                        <div class="progress-bar progress-bar-success" role="progressbar" aria-valuenow="20"
                          aria-valuemin="0" aria-valuemax="100" style="width: 80%">
                          <span class="sr-only">80%</span>
                        </div>
                      </div>
                    </div>
                    <!-- end 5 -->
                    <div class="col-xs-3 col-md-3 text-right">
                      <span class="rstar"></span>4
                    </div>
                    <div class="col-xs-8 col-md-9">
                      <div class="progress">
                        <div class="progress-bar progress-bar-success" role="progressbar" aria-valuenow="20"
                          aria-valuemin="0" aria-valuemax="100" style="width: 60%">
                          <span class="sr-only">60%</span>
                        </div>
                      </div>
                    </div>
                    <!-- end 4 -->
                    <div class="col-xs-3 col-md-3 text-right">
                      <span class="rstar"></span>3
                    </div>
                    <div class="col-xs-8 col-md-9">
                      <div class="progress">
                        <div class="progress-bar progress-bar-info" role="progressbar" aria-valuenow="20"
                          aria-valuemin="0" aria-valuemax="100" style="width: 40%">
                          <span class="sr-only">40%</span>
                        </div>
                      </div>
                    </div>
                    <!-- end 3 -->
                    <div class="col-xs-3 col-md-3 text-right">
                      <span class="rstar"></span>2
                    </div>
                    <div class="col-xs-8 col-md-9">
                      <div class="progress">
                        <div class="progress-bar progress-bar-warning" role="progressbar" aria-valuenow="20"
                          aria-valuemin="0" aria-valuemax="100" style="width: 20%">
                          <span class="sr-only">20%</span>
                        </div>
                      </div>
                    </div>
                    <!-- end 2 -->
                    <div class="col-xs-3 col-md-3 text-right">
                      <span class="rstar"></span>1
                    </div>
                    <div class="col-xs-8 col-md-9">
                      <div class="progress">
                        <div class="progress-bar progress-bar-danger" role="progressbar" aria-valuenow="80"
                          aria-valuemin="0" aria-valuemax="100" style="width: 15%">
                          <span class="sr-only">15%</span>
                        </div>
                      </div>
                    </div>
                    <!-- end 1 -->
                  </div>
                  <!-- end row -->
                </div>
              </div>
            </div>
          </div>
        </div>

      </section>
      <!-- course rating  end -->