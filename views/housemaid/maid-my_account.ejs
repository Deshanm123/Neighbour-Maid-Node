              </div>

            </div>

            <!-- Previous Occupation:Text input-->
            <div class="form-group">
              <label class=" control-label" for="Secondary Occupation (if any)">Previous Occupation (if any) </label>
              <div class="input-group">
                <div class="input-group-addon">
                  <i class="fa fa-briefcase"></i>
                </div>
                <input id="Secondary Occupation (if any)" name="Secondary Occupation (if any)" type="text"
                  placeholder="Secondary Occupation (if any)" class="form-control input-md">
              </div>
            </div>

            <!-- Text input-->
            <div class="form-group">
              <label class="control-label" for="Skills">Skills</label>
              <div class="input-group">
                <div class="input-group-addon">
                  <i class="fa fa-graduation-cap"></i>
                </div>
                <input id="Skills" name="Skills" type="text" placeholder="Skills" class="form-control input-md">
              </div>
            </div>

            <!-- Textarea -->
            <div class="form-group">
              <label class="control-label" for="Overview (max 200 words)">Overview (max 200 words)</label>
              <textarea class="form-control" rows="10" id="Overview (max 200 words)" name="Overview (max 200 words)">Overview
            </textarea>

