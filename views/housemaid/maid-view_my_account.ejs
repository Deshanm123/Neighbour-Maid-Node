            <!-- ajax JavaScript -->
            <!-- <script type="text/javascript" src="/maid_myaccount_ajax.js"></script> -->
            <script>
              //console.log();
              // var birthday = new Date(1998, 04, 01)
              let birthday = '<%= personalDetails[0].uDOB %>';

              function _calculateAge(inputBirthday) {
                // birthday is a date
                let birthday = new Date(inputBirthday);
                var ageDifMs = Date.now() - birthday.getTime();
                var ageDate = new Date(ageDifMs); // miliseconds from epoch
                return Math.abs(ageDate.getUTCFullYear() - 1970);
              }
              let age = _calculateAge('<%= personalDetails[0].uDOB %>');
              // console.log(age);
              document.getElementById('age').innerHTML = age;
            </script>

</body>

